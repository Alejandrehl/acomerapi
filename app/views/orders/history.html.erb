  <h3 class="text-center" style="width:100%;float:left;">Ordenes Completadas </h3>
      <div class="container table-responsive pt-5 pb-5">
      <div class="text-center mb-5">
      </div>
      <%if @orders.empty?%>
      <div> 
        <div class="card card-warning text-center">
          <h4 class="text-center">
            Aun no tienes ordenes completadas.
          </h4>
        </div>
      </div>
      <%else%>
      <table id="datatable-1" class="table table-datatable table-striped table-hover">
        <thead>
          <tr>
            <th>Numero de Pedido</th>
            <th>Total</th>
            <th>Estado</th>
            <th>Fechas</th>
            <th>Mesa</th>
          </tr>
        </thead>
        <tbody>
          <% @orders.each do |dl| %>
            <tr>
              <td>
                <%= dl.id %>
              </td>
              <td>
                $<%= dl.total %>
              </td>
              <td>
                <%= "Completada" %>
              </td>
              <td>
                <%= readable_fulltime(dl.created_at) %>
              </td>
              <td>
                <%= dl.tables_id %>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <br> <br> <br>
    <% end %>